# Configuration de la base de données
DB_SERVER_NAME=localhost\SQLEXPRESS02
DB_NAME=electionapp

# Configuration des API externes
GEMINI_API_KEY=your_gemini_api_key_here

# Configuration Django
DJANGO_SECRET_KEY=your_django_secret_key_here
DEBUG=True

# Configuration email
EMAIL_BACKEND=sendgrid_backend.SendgridBackend
DEFAULT_FROM_EMAIL=your_email@example.com

# Configuration des URLs
SITE_URL=http://127.0.0.1:8000
LOCAL_TUNNEL_URL=https://yourproject.loca.lt

# Configuration CORS
ALLOWED_HOSTS=localhost,127.0.0.1,yourproject.loca.lt
CSRF_TRUSTED_ORIGINS=https://yourproject.loca.lt,http://localhost:8000,http://localhost:3000
CORS_ALLOWED_ORIGINS=http://localhost:8000,http://127.0.0.1:8000,http://localhost:8080,http://localhost:3000

# Configuration de session
SESSION_COOKIE_AGE=2592000
SESSION_EXPIRE_AT_BROWSER_CLOSE=False
SESSION_SAVE_EVERY_REQUEST=True

# Configuration de sécurité
CSRF_COOKIE_SECURE=True

